/* pages/upload_img/upload_img.wxss */
@import '../component/model/model';

page{
  background: #f5f5f5
}
.uploading_photos{
  width: 100%;
  margin-bottom: 10rpx;
}
.delet_photo{
  width: 45rpx;
  height: 45rpx;
  position: absolute;
  right: 0;
  top: 0;
}
.upload_img{
  width: 100%;
  height: 410rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.upload_img image{
  width: 180rpx;
  height: 150rpx;
}
/*
*单图
*/
.thumb{
  width: 100%;
}